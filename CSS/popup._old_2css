#popup {
    left: 0;
    position: absolute;
    top: 0;
    width: 300px;
    z-index: 1001;
  color: #000;
  font-size: 20px;
  height: auto;
}

.content-popup {
    position: absolute;
    left: -230px;
    padding:10px;
    width:200px;
    border-radius:4px;
    background-color:#fff;
    box-shadow: 0 2px 5px #666666;
    color: #000;
 font-size: 20px;
   height: auto;
}

.content-popup h2 {
    color:#48484B;
    border-bottom: 1px solid #48484B;
    margin-top: 0;
    padding-bottom: 4px;
    color: #000;
    font-size: 20px;
    height: auto;
}

.popup-overlay {
    left: 0;
    position: absolute;
    top: 0;
    width: 100%;
    z-index: 999;
    display:none;
    background-color: #777777;
    cursor: pointer;
    opacity: 0.7;
    color: #000;
    font-size: 20px;
    height: auto;
}

.close {
    position: absolute;
    right: 15px;
    height: auto;
}
